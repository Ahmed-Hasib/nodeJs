<html>
<head>
<style>
    body{
        background-color:#666;
    }
    h1{
        text-align:center;
        margin-top:100px;
        font-size:40pt;
        color:white;
        
    }
    h2{
        text-align:center;
        font-size:30pt;
        color:white;
    }
    ul li{
        list-style:none;
        color:white;
        font-size:12pt;
    }
    ul{margin-left:45%;}
</style>
</head>
    
    <body>
        <h1>Welcome the profile of <br><%= person%></h1>
        <h2>Age: <%= data.age%><br>Job: <%= data.job%></h2>
        <h2>Hobbies are:</h2>
        <ul>
          <% data.hobbies.forEach(function(item){ %>
            <li><%=item%></li>  
          <% }); %>
        </ul>
        
        <h2>You can't find any website better than nodejs</h2>
        
    </body>
</html>